{
  "name": "danivyit-com-web-app",
  "version": "1.0.0",
  "description": "A react app for danivyit.com.",
  "main": "index.js",
  "scripts": {
    "build-dev": "cp public/* dist && \"./node_modules/.bin/webpack\" --mode development --watch --config webpack.config.js",
    "build-prod": "cp public/* dist && \"./node_modules/.bin/webpack\" --mode production --config webpack.config.js",
    "host": "cd dist/ && npx http-server && cd ..",
    "lint": "./node_modules/.bin/tslint --config tslint.json --project tsconfig.json 'src/**/*.tsx'",
    "test": "./node_modules/.bin/jest --config jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/divy4/danivyit-com-web-app.git"
  },
  "author": "Dan Ivy (https://github.com/divy4)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/divy4/danivyit-com-web-app/issues"
  },
  "homepage": "https://github.com/divy4/danivyit-com-web-app#readme",
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "awesome-typescript-loader": "^5.2.0",
    "css-loader": "^1.0.0",
    "jest": "^23.5.0",
    "style-loader": "^0.22.1",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^3.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  }
}
