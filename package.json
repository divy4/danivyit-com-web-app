{
  "name": "danivyit-com-web-app",
  "version": "1.0.0",
  "description": "A react app for danivyit.com.",
  "main": "index.js",
  "scripts": {
    "build-dev": "rm -rf dist/* && \"./node_modules/.bin/webpack\" --mode development --watch --config webpack.config.js",
    "build-prod": "rm -rf dist/ && \"./node_modules/.bin/webpack\" --mode production --config webpack.config.js",
    "host": "cd dist/ && npx http-server && cd ..",
    "host-local": "cd dist/ && npx http-server -a localhost -a 127.0.0.1 && cd ..",
    "lint": "./node_modules/.bin/tslint --config tslint.json --project tsconfig.json src/**/*.tsx",
    "test": "./node_modules/.bin/jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/divy4/danivyit-com-web-app.git"
  },
  "author": "Dan Ivy (https://github.com/divy4)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/divy4/danivyit-com-web-app/issues"
  },
  "homepage": "https://github.com/divy4/danivyit-com-web-app#readme",
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "awesome-typescript-loader": "^5.2.0",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "style-loader": "^0.22.1",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "ttf-loader": "^1.0.2",
    "typescript": "^3.0.1",
    "typings-for-css-modules-loader": "^1.7.0",
    "url-loader": "^1.1.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/react": "^16.4.9",
    "@types/react-bootstrap": "^0.32.11",
    "@types/react-dom": "^16.0.7",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.6",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "react": "^16.4.2",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.2"
